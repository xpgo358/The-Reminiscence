{
    "model": {
        "type": "minecraft:condition",
        "property": "minecraft:using_item",
        "on_false": {
            "type": "minecraft:model",
            "model": "minecraft:item/dried_kelp"
        },
        "on_true": {
            "property": "minecraft:use_duration",
            "scale": 0.03,
            "type": "minecraft:range_dispatch",
            "entries": [
                {
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/food/dried_kelp/dried_kelp1"
                    },
                    "threshold": 0.2
                },
                {
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/food/dried_kelp/dried_kelp3"
                    },
                    "threshold": 0.4
                }
            ],
            "fallback": {
                "type": "minecraft:model",
                "model": "minecraft:item/dried_kelp"
            }
        }
    }
}